datasource db {
  provider = "mysql"
  url      = env("DB_URL")
}

// ~ ~ ~ ~ ~ ~ ~ ~ ~ ~ ~ ~ ~ ~ ~ ~ ~ ~ ~ ~ ~ ~ ~ ~ ~ ~ ~ ~ ~ ~ ~

generator client {
  provider = "prisma-client-js"
}

// ~ ~ ~ ~ ~ ~ ~ ~ ~ ~ ~ ~ ~ ~ ~ ~ ~ ~ ~ ~ ~ ~ ~ ~ ~ ~ ~ ~ ~ ~ ~

model App {
  id        Int      @id @default(autoincrement())
  asaps     Asap[]
  createdAt DateTime @default(now())
  updatedAt DateTime @default(now()) @updatedAt
}

// ~ ~ ~ ~ ~ ~ ~ ~ ~ ~ ~ ~ ~ ~ ~ ~ ~ ~ ~ ~ ~ ~ ~ ~ ~ ~ ~ ~ ~ ~ ~

model Asap {
  id        Int      @id @default(autoincrement())
  weee      String?
  app       App?     @relation(fields: [appid], references: [id])
  appid     Int?
  createdAt DateTime @default(now())
  updatedAt DateTime @default(now()) @updatedAt
}

// ~ ~ ~ ~ ~ ~ ~ ~ ~ ~ ~ ~ ~ ~ ~ ~ ~ ~ ~ ~ ~ ~ ~ ~ ~ ~ ~ ~ ~ ~ ~
